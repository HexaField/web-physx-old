{
  "compilerOptions": {
    "strict": false,
    "outDir": "dist",
    "target": "es5",
    "module": "esnext",
    "types": [
      "node",
      "jest",
      "puppeteer",
      "vite/client"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
